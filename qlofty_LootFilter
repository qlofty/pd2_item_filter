//- WORLDSTONE SHARD AND LARZUK BOX ---
ItemDisplay[wss QTY>1]: %MAP-4A%%NL%%PURPLE%|%WHITE%-----------------------------%PURPLE%|%NL%    %PURPLE%WORLDSTAR%WHITE% ! !%WHITE% x %QTY%%NL%%PURPLE%|%WHITE%-----------------------------%PURPLE%|{%TAN%Note%WHITE%: Use it to %RED%Corrupt %WHITE%an Item}// Quantity tag if QTY greater than 1
ItemDisplay[lbox QTY>1]: ItemDisplay[lbox]: %BORDER-62%%RED%|%WHITE%-----------------------%RED%|%NL%%ORANGE%* %GOLD%PUZZLEBOX%ORANGE% *%NL%%RED%|%WHITE%-----------------------%RED%|

ItemDisplay[wss]:   %MAP-4A%%NL%%PURPLE%|%WHITE%-----------------------------%PURPLE%|%NL%    %PURPLE%WORLDSTAR%WHITE% ! !%WHITE% x %QTY%%NL%%PURPLE%|%WHITE%-----------------------------%PURPLE%|{%TAN%Note%WHITE%: Use it to %RED%Corrupt %WHITE%an Item}
ItemDisplay[lbox]: ItemDisplay[lbox]: %BORDER-62%%RED%|%WHITE%-----------------------%RED%|%NL%%ORANGE%* %GOLD%PUZZLEBOX%ORANGE% *%NL%%RED%|%WHITE%-----------------------%RED%|

//- MAP CRAFTING ----------------------
ItemDisplay[(imra OR imma OR scou OR rera OR upma) QTY>1]: %NAME% %WHITE%x%QTY%%CONTINUE%// Quantity tag if QTY greater than 1

ItemDisplay[imra]: %RED%%NAME% %GRAY%(Create rare map){%TAN%White map + Jewel + Thul or greater}%DOT-68%// Zakarum Orb - Create rare map
ItemDisplay[imma]: %RED%%NAME% %GRAY%(Create magic map){%TAN%White map + Jewel}%DOT-68%// Arcane Orb - Create magic map
ItemDisplay[scou]: %RED%%NAME% %GRAY%(Downgrade map to white){%TAN%Magic/rare map + Hel rune}%DOT-68%// Orb of Destruction - Strip map properties
ItemDisplay[rera]: %RED%%NAME% %GRAY%(Reroll rare map){%TAN%Rare map + Perfect gem + Thul or greater}%DOT-68%// Horadrim Orb - Reroll rare map
ItemDisplay[upma]: %RED%%NAME% %GRAY%(Upgrade map: Magic to rare){%TAN%Magic map + Perfect Skull + Thul or greater}%DOT-68%// Angelic Orb - Upgrade magic map to rare

//- MAP BASES -------------------------
ItemDisplay[t11 NMAG]: %NAME%%MAP-68%// Plains of Torment
