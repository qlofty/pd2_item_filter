ItemDisplay[gpys]: %YELLOW%+%WHITE%Perfect %YELLOW%Topaz x%QTY%%WHITE%%DOT-20%{%WHITE%Shields: Lightning Resist +40%%NL%Armor/Helms: 24% Better Chance of Getting Magic Items%NL%Weapons: Adds 1-40 lightning damage%NL%} // Stacked Perfect Topaz

//Runes
ItemDisplay[RUNE<15 CLVL>0]: %PX-62%%YELLOW%* %ORANGE%%RUNENAME% Rune %GRAY%[%TAN%%RUNENUM%%GRAY%] x %WHITE%%QTY%%CONTINUE%
ItemDisplay[RUNE=15 OR RUNE=16 OR RUNE=17 OR RUNE=18 OR RUNE=19]: %PX-62%%NL%%BLUE%*   %RUNENAME% %ORANGE%Rune %WHITE%[%BLUE%%RUNENUM%%WHITE%]   %BLUE%*%CONTINUE%
ItemDisplay[RUNE>19 AND RUNE<26]: %DOT-62%%NL% %BLUE%| %YELLOW%* %GREEN%* %RED%*   %RUNENAME% %RED%Rune   %RED%* %GREEN%* %YELLOW%* %BLUE%| %NL%%WHITE%[%RED%%RUNENUM%%WHITE%]%CONTINUE%
ItemDisplay[RUNE>25]: %BORDER-62%%NL%%GREEN%|%PURPLE%-%GREEN%| %PURPLE%* %ORANGE%* %PURPLE%*    %RUNENAME% %PURPLE%Rune    %PURPLE%* %ORANGE%* %PURPLE%* %GREEN%|%PURPLE%-%GREEN%| %NL%%WHITE%[%GREEN%%RUNENUM%%WHITE%]  %CONTINUE%

