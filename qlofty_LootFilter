//- SCROLLS AND TOMES -----------------
ItemDisplay[tbk]: %NAME%
ItemDisplay[ibk]: %NAME%
ItemDisplay[tsc CLVL<85]: TP
ItemDisplay[tsc CLVL>84]:
ItemDisplay[isc CLVL<85]: ID
ItemDisplay[isc CLVL>84]:

//- KEY/ARROW/BOLT --------------------
ItemDisplay[key QTY>1]: Keys %WHITE%x%QTY%%CONTINUE%// Quantity tag if QTY greater than 1
ItemDisplay[key CLVL<85]: %NAME%
ItemDisplay[key CLVL>84]:

ItemDisplay[aqv CLVL<15]: %NAME%// Arrow
ItemDisplay[aqv CLVL>14]:
ItemDisplay[cqv CLVL<15]: %NAME%// Bolts
ItemDisplay[cqv CLVL>14]:

//- DRINKING POTIONS ------------------
ItemDisplay[hp1 CLVL<15]: %RED%HP1
ItemDisplay[hp1 CLVL>14]:
ItemDisplay[hp2 CLVL<25]: %RED%HP2
ItemDisplay[hp2 CLVL>24]:
ItemDisplay[hp3 CLVL<40]: %RED%HP3
ItemDisplay[hp3 CLVL>39]:
ItemDisplay[hp4 CLVL<80]: %RED%HP4
ItemDisplay[hp4 CLVL>79]:
ItemDisplay[hp5]: %WHITE%*%RED% HP %WHITE%*

ItemDisplay[mp1 CLVL<15]: %BLUE%MP1
ItemDisplay[mp1 CLVL>14]:
ItemDisplay[mp2 CLVL<25]: %BLUE%MP2
ItemDisplay[mp2 CLVL>24]:
ItemDisplay[mp3 CLVL<40]: %BLUE%MP3
ItemDisplay[mp3 CLVL>39]:
ItemDisplay[mp4 CLVL<80]: %BLUE%MP4
ItemDisplay[mp4 CLVL>79]:
ItemDisplay[mp5]: %WHITE%*%BLUE% MP %WHITE%*

ItemDisplay[rvs CLVL>80]:
ItemDisplay[rvs CLVL<80]: %PURPLE%Half Rejuv
ItemDisplay[rvl]: %DOT-62%%WHITE%* %PURPLE%FULL %WHITE%Rejuv %PURPLE% 

ItemDisplay[vps CLVL<15]: Stamina
ItemDisplay[vps CLVL>14]:
ItemDisplay[yps CLVL<75]: Antidote
ItemDisplay[yps CLVL>74]: Antidote
ItemDisplay[wms CLVL<75]: Thaw
ItemDisplay[wms CLVL>74]: Thaw

//- THROWING POTIONS ------------------
ItemDisplay[gpl CLVL<15]: Strangling
ItemDisplay[gpl CLVL>14]:
ItemDisplay[gpm CLVL<15]: Choking
ItemDisplay[gpm CLVL>14]:
ItemDisplay[gps CLVL<15]: Rancid
ItemDisplay[gps CLVL>14]:

ItemDisplay[opl CLVL<15]: Fulminating
ItemDisplay[opl CLVL>14]:
ItemDisplay[opm CLVL<15]: Exploding
ItemDisplay[opm CLVL>14]:
ItemDisplay[ops CLVL<15]: Oil
ItemDisplay[ops CLVL>14]:

//############################################################################################
//- RUNES ---------------- RUNES ---------------- RUNES ---------------- RUNES ---------------
//############################################################################################

ItemDisplay[RUNE<15 CLVL>0]: %PX-62%%YELLOW%* %ORANGE%%RUNENAME% Rune %GRAY%[%TAN%%RUNENUM%%GRAY%] x %WHITE%%QTY%%CONTINUE%
ItemDisplay[RUNE=15 OR RUNE=16 OR RUNE=17 OR RUNE=18 OR RUNE=19]: %PX-62%%NL%%BLUE%*   %RUNENAME% %ORANGE%Rune %WHITE%[%BLUE%%RUNENUM%%WHITE%]   %BLUE%*%CONTINUE%
ItemDisplay[RUNE>19 AND RUNE<26]: %DOT-62%%NL% %BLUE%| %YELLOW%* %GREEN%* %RED%*   %RUNENAME% %RED%Rune   %RED%* %GREEN%* %YELLOW%* %BLUE%| %NL%%WHITE%[%RED%%RUNENUM%%WHITE%]%CONTINUE%
ItemDisplay[RUNE>25]: %BORDER-62%%NL%%GREEN%|%PURPLE%-%GREEN%| %PURPLE%* %ORANGE%* %PURPLE%*    %RUNENAME% %PURPLE%Rune    %PURPLE%* %ORANGE%* %PURPLE%* %GREEN%|%PURPLE%-%GREEN%| %NL%%WHITE%[%GREEN%%RUNENUM%%WHITE%]  %CONTINUE%

ItemDisplay[r01]: %NAME%{%TAN%3 El + Key = Eld%NL%} // El Rune
ItemDisplay[r01s]: %NAME%{%TAN%3 El + Key = Eld%NL%%NL%%WHITE%All: +1 Light Radius%NL%Shields: +15 Defense%NL%Armor/Helms: +15 Defense%NL%Weapons: +50 Attack Rating%NL%} // El Rune
ItemDisplay[r02]: %NAME%{%TAN%3 Eld + Key = Tir%NL%} // Eld Rune
ItemDisplay[r02s]: %NAME%{%TAN%3 Eld + Key = Tir%NL%%NL%%WHITE%Shields: +7% Block%NL%Armor/Helms: -15% Stam Drain%NL%Weapons: +75% Dmg & +50 Attack Rating vs Undead%NL%} // Eld Rune
ItemDisplay[r03]: %NAME%{%TAN%3 Tir + Key = Nef%NL%} // Tir Rune
ItemDisplay[r03s]: %NAME%{%TAN%3 Tir + Key = Nef%NL%%NL%%WHITE%Shields: +2 Mana Per Kill%NL%Armor/Helms: +2 Mana Per Kill%NL%Weapons: +2 Mana Per Kill%NL%} // Tir Rune
ItemDisplay[r04]: %NAME%{%TAN%3 Nef + Key = Eth%NL%} // Nef Rune
ItemDisplay[r04s]: %NAME%{%TAN%3 Nef + Key = Eth%NL%%NL%%WHITE%Shields: +30 Defense vs Missile%NL%Armor/Helms: +30 Defense vs Missile%NL%Weapons: Knockback%NL%} // Nef Rune
ItemDisplay[r05]: %NAME%{%TAN%3 Eth + Key = Ith%NL%} // Eth Rune
ItemDisplay[r05s]: %NAME%{%TAN%3 Eth + Key = Ith%NL%%NL%%WHITE%Shields: Regen Mana 15%%NL%Armor/Helms: Regen Mana 15%%NL%Weapons: -25% Target Defense%NL%} // Eth Rune
ItemDisplay[r06]: %NAME%{%TAN%3 Ith + Key = Tal%NL%} // Ith Rune
ItemDisplay[r06s]: %NAME%{%TAN%3 Ith + Key = Tal%NL%%NL%%WHITE%Shields: 15% Dmg Taken Gained as Mana%NL%Armor/Helms: 15% Dmg Taken Gained as Mana%NL%Weapons: +9 Max Dmg%NL%} // Ith Rune
ItemDisplay[r07]: %NAME%{%TAN%3 Tal + Key = Ral%NL%} // Tal Rune
ItemDisplay[r07s]: %NAME%{%TAN%3 Tal + Key = Ral%NL%%NL%%WHITE%Shields: Poison Resist +35%%NL%Armor/Helms: Poison Resist +30%%NL%Weapons: +75 poison damage over 5 seconds%NL%} // Tal Rune
ItemDisplay[r08]: %NAME%{%TAN%3 Ral + Key = Ort%NL%} // Ral Rune
ItemDisplay[r08s]: %NAME%{%TAN%3 Ral + Key = Ort%NL%%NL%%WHITE%Shields: Fire Resist +35%%NL%Armor/Helms: Fire Resist +30%%NL%Weapons: Adds 5-30 fire damage%NL%} // Ral Rune
ItemDisplay[r09]: %NAME%{%TAN%3 Ort + Key = Thul%NL%} // Ort Rune
ItemDisplay[r09s]: %NAME%{%TAN%3 Ort + Key = Thul%NL%%NL%%WHITE%Shields: Lightning Resist +35%%NL%Armor/Helms: Lightning Resist +30%%NL%Weapons: Adds 1-50 lightning dmg%NL%} // Ort Rune
ItemDisplay[r10]: %NAME%{%TAN%3 Thul + Key = Amn%NL%} // Thul Rune
ItemDisplay[r10s]: %NAME%{%TAN%3 Thul + Key = Amn%NL%%NL%%WHITE%Shields: Cold Resist +35%%NL%Armor/Helms: Cold Resist +30%%NL%Weapons: Adds 3-14 cold damage%NL%} // Thul Rune
ItemDisplay[r11]: %NAME%{%TAN%3 Amn + Key = Sol%NL%} // Amn Rune
ItemDisplay[r11s]: %NAME%{%TAN%3 Amn + Key = Sol%NL%%NL%%WHITE%Shields: Attacker Takes Damage of 14%NL%Armor/Helms: Attacker Takes Damage of 14%NL%Weapons: 7% Life steal%NL%} // Stacked Amn Rune
ItemDisplay[r12]: %NAME%{%TAN%3 Sol + Key = Shael%NL%} // Sol Rune
ItemDisplay[r12s]: %NAME%{%TAN%3 Sol + Key = Shael%NL%%NL%%WHITE%Shields: Damage Reduced by 7%NL%Armor/Helms: Damage Reduced by 7%NL%Weapons: +9 to Minimum Damage%NL%} // Stacked Sol Rune
ItemDisplay[r13]: %NAME%{%TAN%3 Shael + Key = Dol%NL%} // Shael Rune
ItemDisplay[r13s]: %NAME%{%TAN%3 Shael + Key = Dol%NL%%NL%%WHITE%Shields: 20% Faster Block Rate%NL%Armor/Helms: 20% Faster Hit Recovery%NL%Weapons: +20% Attack Speed%NL%} // Stacked Shael Rune
ItemDisplay[r14]: %NAME%{%TAN%3 Dol + Key = Hel%NL%} // Dol Rune
ItemDisplay[r14s]: %NAME%{%TAN%3 Dol + Key = Hel%NL%%NL%%WHITE%Shields: Replenish Life +7%NL%Armor/Helms: Replenish Life +7%NL%Weapons: Hit Causes Monster to Flee 25%%NL%} // Stacked Dol Rune
ItemDisplay[r15]: %NAME%{%TAN%3 Hel + Key = Io%NL%} // Hel Rune
ItemDisplay[r15s]: %NAME%{%TAN%3 Hel + Key = Io%NL%%NL%%WHITE%Shields: Requirements -15%%NL%Armor/Helms: Requirements -15%%NL%Weapons: Requirements -20%%NL%} // Stacked Hel Rune
ItemDisplay[r16]: %NAME%{%TAN%3 Io + Key = Lum%NL%} // Io Rune
ItemDisplay[r16s]: %NAME%{%TAN%3 Io + Key = Lum%NL%%NL%%WHITE%Shields: +10 Vitality%NL%Armor/Helms: +10 Vitality%NL%Weapons: +10 Vitality%NL%} // Stacked Io Rune
ItemDisplay[r17]: %NAME%{%TAN%3 Lum + Key = Ko%NL%} // Lum Rune
ItemDisplay[r17s]: %NAME%{%TAN%3 Lum + Key = Ko%NL%%NL%%WHITE%Shields: +10 Energy%NL%Armor/Helms: +10 Energy%NL%Weapons: +10 Energy%NL%} // Stacked Lum Rune
