//- ACT 1 -----------------------------
ItemDisplay[bks]: %GOLD%%NAME% %GRAY%(Talk to Akara)// Scroll of Inifuss
ItemDisplay[bkd]: %GOLD%%NAME% %GRAY%(Go to Stony Field)// Scroll of Inifuss, deciphered
ItemDisplay[leg]: %GOLD%%NAME%// Wirt's Leg
ItemDisplay[hdm]: %GOLD%%NAME% %GRAY%(Give to Charsi)// Horadric Malus

//- ACT 2 -----------------------------
ItemDisplay[ass]: %GOLD%Book of Skill %GRAY%(Right click, +1 skill)// Book of Skill
ItemDisplay[tr1]:// Horadric Scroll
ItemDisplay[box]: %GOLD%%NAME%{%TAN%Shield: Tal + Amn + P Ruby%NL%Armor: Tal + Thul + P Topaz%NL%Weapon: Ral + Amn + P Amethyst%NL%Socket:}// Horadric Cube
ItemDisplay[msf]: %GOLD%%NAME% %GRAY%(Cube with Amulet of the Viper)// Staff of Kings
ItemDisplay[vip]: %GOLD%%NAME% %GRAY%(Cube with Staff of Kings)// Viper Amulet
ItemDisplay[hst]: %GOLD%%NAME% %GRAY%(Use in Tal'rasha Tomb Orifice)// Horadric Staff

//- ACT 3 -----------------------------
ItemDisplay[j34]: %GOLD%%NAME% %GRAY%%MAP-00%(Give to Meshif)// A Jade Figurine
ItemDisplay[g34]: %GOLD%%NAME% %GRAY%(Give to Alkor)// The Golden Bird
ItemDisplay[xyz]: %GOLD%Potion of Life %GRAY%(+20 HP)// Potion of Life
ItemDisplay[g33]: %GOLD%%NAME% %GRAY%(Give to Ormus)// Gidbinn
ItemDisplay[bbb]: %GOLD%%NAME% %GRAY%(Give to Alkor, +5 stats)// Lam Esen’s Tome
ItemDisplay[qf1]: %GOLD%%NAME% %GRAY%(Flail, Eye, Heart, Brain in Cube)// Khalim’s Flail
ItemDisplay[qey]: %GOLD%%NAME% %GRAY%(Flail, Eye, Heart, Brain in Cube)// Khalim’s Eye
ItemDisplay[qhr]: %GOLD%%NAME% %GRAY%(Flail, Eye, Heart, Brain in Cube)// Khalim’s Heart
ItemDisplay[qbr]: %GOLD%%NAME% %GRAY%(Flail, Eye, Heart, Brain in Cube)// Khalim’s Brain
ItemDisplay[qf2]: %GOLD%%NAME% %GRAY%(Hit Travincal Orb)// Khalim’s Will
ItemDisplay[mss]: %GOLD%%NAME% %GRAY%(Click Hellforge)// Mephisto’s Soulstone

//- ACT 4 -----------------------------
ItemDisplay[hfh]: %GOLD%%NAME% %GRAY%(Swing at Hellforge)// Hellforge Hammer

//- ACT 5 -----------------------------
ItemDisplay[ice]: %GOLD%%NAME% %GRAY%(Click on Frozen Anya)// Malah’s Potion
ItemDisplay[tr2]: %GOLD%%NAME% %GRAY%(+10 Resists)// Scroll of Resistance

//- PANDEMONIUM KEYS ------------------
ItemDisplay[pk1]: %MAP-68% %RED%|%WHITE%-----------------------%RED%|%NL%    %GOLD%Key %WHITE%of %RED%Terror %WHITE%x%QTY%%NL% %RED%|%WHITE%-----------------------%RED%|{%TAN%Key %WHITE%from: %RED%Countess %WHITE%or %RED%Blood Raven}
ItemDisplay[pk2]: %MAP-68% %BLUE%|%WHITE%-----------------------%BLUE%|%NL%  %GOLD%Key %WHITE%of %BLUE%Hate %WHITE%x%QTY%%NL% %BLUE%|%WHITE%-----------------------%BLUE%|{%TAN%Key %WHITE%from: %BLUE%Summoner %WHITE%or %BLUE%Bloodwitch}
ItemDisplay[pk3]: %MAP-68% %PURPLE%|%WHITE%-----------------------------%PURPLE%|%NL%    %GOLD%Key %WHITE%of %PURPLE%Destruction %WHITE%x%QTY%%NL% %PURPLE%|%WHITE%-----------------------------%PURPLE%|{%TAN%Key %WHITE%from: %PURPLE%Nihlathak %WHITE%or %PURPLE%Izual}


//- PANDEMONIUM ORGANS ----------------
ItemDisplay[(dhn OR bey OR mbr)]: %BORDER-62%%GOLD%-%BLUE%=%GOLD%-%BLUE%=%GOLD%-%BLUE%=%NL%%ORANGE%** %NAME% %ORANGE%**%NL%%BLUE%=%GOLD%-%BLUE%=%GOLD%-%BLUE%=%GOLD%-

ItemDisplay[mbr]: %YELLOW%+ %ORANGE%%NAME%%MAP-68%// Mephisto's Brain
ItemDisplay[dhn]: %RED%+ %ORANGE%%NAME%%MAP-68%// Diablo's Horn
ItemDisplay[bey]: %GREEN%+ %ORANGE%%NAME%%MAP-68%// Baal's Eye

//- TOKEN OF ABSOLUTION SYSTEM --------
ItemDisplay[tes]: %BLUE%%MAP-68%%NAME%{%TAN%Essence %WHITE%from: %BLUE%Andariel %WHITE%or %BLUE%Duriel}%CONTINUE%
ItemDisplay[ceh]: %WHITE%%MAP-68%%NAME%{%TAN%Essence %WHITE%from: %TAN%Mephisto}%CONTINUE%
ItemDisplay[bet]: %RED%%MAP-68%%NAME%{%TAN%Essence %WHITE%from: %RED%Diablo}%CONTINUE%
ItemDisplay[fed]: %GREEN%%MAP-68%%NAME%{%TAN%Essence %WHITE%from: %DARK_GREEN%Baal}%CONTINUE%
ItemDisplay[toa]: %BORDER-62%%GOLD%-%BLUE%=%GOLD%-%BLUE%=%GOLD%-%BLUE%=%NL%%ORANGE%** %NAME% %ORANGE%**%NL%%BLUE%=%GOLD%-%BLUE%=%GOLD%-%BLUE%=%GOLD%-

